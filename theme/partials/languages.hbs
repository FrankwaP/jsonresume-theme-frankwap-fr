{{#if resume.languages.length}}
<section class="section">
  <header>
    <h2 class='section-title'>Langues</h2>
  </header>
  <section id="languages">
    {{#each resume.languages}}
    <div class="display">
      {{#if language}}
      <h3 class="language">
        {{language}}
      </h3>
      {{/if}}
      <div class="item">
        {{#if fluency}}
        		{{#fluencyDiv fluency}}
		        {{/fluencyDiv}}
        {{/if}}
      </div>
    </div>
    {{/each}}
  </section>
</section>
{{/if}}
